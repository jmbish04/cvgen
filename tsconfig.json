{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["esnext"],
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "types": [
      "@cloudflare/workers-types",
      "jest"
    ]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
